#!/bin/sh

if [ "$#" -ne 1 ]; then
	echo "Usage: deldevtrace <org>"
	exit 1
fi

id=`sf --org=$1 toolingQuery "select id, tracedentity.name from traceflag" | jq -M '.records | map(select(.TracedEntity.Name == "Trifecta Developers")) | .[].Id' | cut -d'"' -f2`
sf --org=$1 toolingDelete $id | jq '.[]'

